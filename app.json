{
  "expo": {
    "name": "pokemoncard",
    "slug": "pokemoncard",
    "version": "1.0.0",
    "scheme": "pokemoncard",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pokemoncard.app",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "pokemoncard"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pokemoncard.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "pokemoncard"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
